{
  "name": "@vunio/ui",
  "description": "@vunio ui library",
  "version": "1.3.0",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./style.css": "./dist/style.css",
    "./*": "./dist/*"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dpbs-715/vunio"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "scripts": {
    "build": "rimraf dist && vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf .turbo node_modules dist",
    "runPublish": "pnpm publish"
  },
  "devDependencies": {
    "@vunio/typescript-config": "workspace:*",
    "@element-plus/icons-vue": "catalog:ui",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:build",
    "@vitejs/plugin-vue-jsx": "catalog:build",
    "@vitest/coverage-v8": "catalog:dev",
    "@vue/test-utils": "catalog:dev",
    "element-plus": "catalog:ui",
    "jsdom": "catalog:dev",
    "sass": "catalog:styles",
    "typescript": "catalog:",
    "unplugin-vue-components": "catalog:build",
    "vite": "catalog:build",
    "vite-plugin-dts": "catalog:build",
    "vitest": "catalog:dev",
    "vue": "catalog:",
    "vue-draggable-plus": "catalog:ui"
  },
  "peerDependencies": {
    "@element-plus/icons-vue": "^2.3.0",
    "element-plus": "^2.6.1",
    "vue": "^3.5.0"
  },
  "dependencies": {
    "@codemirror/autocomplete": "catalog:ui",
    "@codemirror/commands": "catalog:ui",
    "@codemirror/language": "catalog:ui",
    "@codemirror/lint": "catalog:ui",
    "@codemirror/search": "catalog:ui",
    "@codemirror/state": "catalog:ui",
    "@codemirror/view": "catalog:ui",
    "codemirror": "catalog:ui",
    "jsep": "catalog:ui"
  }
}
