{
  "name": "dlib-utils",
  "description": "@DLib utils library",
  "version": "1.2.4",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf .turbo node_modules dist",
    "runPublish": "pnpm publish"
  },
  "devDependencies": {
    "@DLib/typescript-config": "workspace:*",
    "@rollup/plugin-json": "catalog:build",
    "@vitest/coverage-v8": "catalog:dev",
    "rollup": "catalog:build",
    "rollup-plugin-dts": "catalog:build",
    "rollup-plugin-esbuild": "catalog:build",
    "vitest": "catalog:dev"
  }
}
